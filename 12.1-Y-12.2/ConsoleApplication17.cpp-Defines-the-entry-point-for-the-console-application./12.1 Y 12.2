#include "stdafx.h"
#include "iostream"
#include "conio.h"
using namespace std;
template<class T> T min(T a, T b)
{
	if (a <= b)
		return a;
	else
		return b;
}
template<class T> T max(T a, T b)
{
	if (a >= b)
		return a;
	else
		return b;
}
void main()
{
	int ea = 1, eb = 5;
	cout << "int: " << min(ea,eb) << endl;
	cout << "int: " << max(ea, eb) << endl;
	long la = 1000, lb = 5000;
	cout << "long: " << min(la, lb) << endl;
	cout << "long: " << max(la, lb) << endl;
	char ca = 'a', cb = 'x';
	cout << "char: " << min(ca, cb) << endl;
	cout << "char: " << max(ca, cb) << endl;
	double da = 423.654, db = 789.12;
	cout << "double: " << min(da, db) << endl;
	cout << "double: " << max(da, db) << endl;
	_getch();
}
